{
  "name": "NimbusBase RSS Reader",
  "description": "A RSS Reader Based on NimbusBase",
  "version": "1.0",
  "manifest_version": 2,  
  "icons": { "16": "rss-16.png", "128": "rss-128.png" },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
  	"tabs",
    "http://*/*", 
    "https://*/*",
    "unlimitedStorage"
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'; img-src: 'self' http://*/* "
}